{"code":"import React, { useState } from \"react\";\r\nimport { TextField, Label, Icon, } from \"office-ui-fabric-react\";\r\nimport { alphanumericRegex, emailRegex, zipRegex } from \"../constants/contants\";\r\nconst textFieldStyles = {\r\n    root: {\r\n        \"& .ms-TextField-fieldGroup\": {\r\n            border: \"2px solid #CBD1D7 !important\",\r\n            borderRadius: \"4px\",\r\n            borderColor: \"transparent\",\r\n            \"::after\": {\r\n                borderColor: \"transparent\",\r\n            },\r\n            \":hover\": {\r\n                borderColor: \"transparent\",\r\n            },\r\n        },\r\n    },\r\n};\r\nconst TextInput = ({ label = \"\", value = \"\", onChange, placeholder = \"\", required = true, multiline = false, errorMessage = \"\", length = 200, alphaCheck = false, hideLabel = false, isPreview = false, isEmailCheck = false, isZipCodeCheck = false, type = \"text\", rows = 1, Styles = textFieldStyles, disabled = false, isLink = false }) => {\r\n    const [error, setError] = useState(errorMessage);\r\n    const [showError, setShowError] = useState(!!errorMessage);\r\n    const handleChange = (_, newValue) => {\r\n        if (newValue?.length > length) {\r\n            setError(`This field must be no more than ${length} characters.`);\r\n            setShowError(true);\r\n        }\r\n        else if (alphaCheck) {\r\n            if (newValue && !alphanumericRegex.test(newValue)) {\r\n                setError(`This field must be alphanumeric`);\r\n                setShowError(true);\r\n                setTimeout(() => {\r\n                    setError(\"\");\r\n                    setShowError(false);\r\n                }, 2000);\r\n            }\r\n            else {\r\n                setError(\"\");\r\n                setShowError(false);\r\n                onChange(newValue);\r\n            }\r\n        }\r\n        else {\r\n            setError(\"\");\r\n            setShowError(false);\r\n            onChange(newValue);\r\n        }\r\n    };\r\n    React.useEffect(() => {\r\n        if (value) {\r\n            if (!emailRegex.test(value) && isEmailCheck) {\r\n                setError(\"Email is in invalid format\");\r\n            }\r\n            else if (!zipRegex.test(value) && isZipCodeCheck) {\r\n                setError(\"Invalid Zip Code\");\r\n            }\r\n            else {\r\n                errorMessage = \"\";\r\n                setError(\"\");\r\n            }\r\n        }\r\n        else {\r\n            setError(errorMessage);\r\n        }\r\n    }, [value]);\r\n    React.useEffect(() => {\r\n        if (errorMessage) {\r\n            setError(errorMessage);\r\n            setShowError(true);\r\n        }\r\n    }, [errorMessage]);\r\n    return (React.createElement(\"div\", null,\r\n        !hideLabel ? (React.createElement(Label, { style: { fontSize: '13px', color: '#475055' }, required: required }, label)) : null,\r\n        isPreview ? (React.createElement(React.Fragment, null, multiline ? (React.createElement(React.Fragment, null,\r\n            React.createElement(\"span\", null, value?.replaceAll(decodeURIComponent(\"%0A\"), \"<br>\") || \"\"))) : (React.createElement(\"span\", null, isLink ? (React.createElement(\"a\", { href: value?.startsWith('http://') || value?.startsWith('https://') ? value : `https://${value}`, target: \"_blank\", rel: \"noopener noreferrer\" // This is recommended for security when using target=\"_blank\"\r\n            , style: { cursor: 'pointer', textDecoration: 'underline', color: '#0078d4' } }, value)) : (value))))) : (React.createElement(React.Fragment, null,\r\n            React.createElement(TextField, { value: value, placeholder: placeholder, styles: Styles, validateOnLoad: false, onChange: handleChange, style: {\r\n                    border: \"none !important\",\r\n                }, disabled: disabled, multiline: multiline, type: type, rows: rows }),\r\n            (showError || error) && (React.createElement(\"div\", { style: { color: '#a4262c', fontSize: '12px', display: 'flex', gap: '5px', margin: '2px 0px 0px 0px' } },\r\n                React.createElement(Icon, { style: { marginTop: '2px' }, iconName: \"IncidentTriangle\" }),\r\n                React.createElement(\"span\", null, error || errorMessage)))))));\r\n};\r\nexport default TextInput;\r\n//# sourceMappingURL=TextInput.js.map","references":["C:/Users/Bharath.Kumar/source/repos/SPFxUIToolKit/spfx-ui-toolkit/node_modules/@types/react/index.d.ts","C:/Users/Bharath.Kumar/source/repos/SPFxUIToolKit/spfx-ui-toolkit/node_modules/office-ui-fabric-react/lib/index.d.ts","C:/Users/Bharath.Kumar/source/repos/SPFxUIToolKit/spfx-ui-toolkit/src/constants/contants.ts"],"map":"{\"version\":3,\"file\":\"TextInput.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../src/components/TextInput.tsx\"],\"names\":[],\"mappings\":\"AAAA,OAAO,KAAK,EAAE,EAAE,QAAQ,EAAE,MAAM,OAAO,CAAC;AACxC,OAAO,EACL,SAAS,EACT,KAAK,EACL,IAAI,GAEL,MAAM,wBAAwB,CAAC;AAChC,OAAO,EAAE,iBAAiB,EAAE,UAAU,EAAE,QAAQ,EAAE,MAAM,uBAAuB,CAAC;AAEhF,MAAM,eAAe,GAA8B;IAC/C,IAAI,EAAE;QACJ,4BAA4B,EAAE;YAC5B,MAAM,EAAE,8BAA8B;YACtC,YAAY,EAAE,KAAK;YACnB,WAAW,EAAE,aAAa;YAC1B,SAAS,EAAE;gBACT,WAAW,EAAE,aAAa;aAC3B;YACD,QAAQ,EAAE;gBACR,WAAW,EAAE,aAAa;aAC3B;SACF;KACF;CACF,CAAC;AAEJ,MAAM,SAAS,GAAG,CAAC,EACjB,KAAK,GAAG,EAAE,EACV,KAAK,GAAE,EAAE,EACT,QAAQ,EACR,WAAW,GAAC,EAAE,EACd,QAAQ,GAAG,IAAI,EACf,SAAS,GAAG,KAAK,EACjB,YAAY,GAAG,EAAE,EACjB,MAAM,GAAG,GAAG,EACZ,UAAU,GAAG,KAAK,EAClB,SAAS,GAAG,KAAK,EACjB,SAAS,GAAG,KAAK,EACjB,YAAY,GAAG,KAAK,EACpB,cAAc,GAAG,KAAK,EACtB,IAAI,GAAG,MAAM,EACb,IAAI,GAAG,CAAC,EACR,MAAM,GAAG,eAAe,EACxB,QAAQ,GAAG,KAAK,EAChB,MAAM,GAAE,KAAK,EACd,EAAE,EAAE;IACH,MAAM,CAAC,KAAK,EAAE,QAAQ,CAAC,GAAG,QAAQ,CAAC,YAAY,CAAC,CAAC;IACjD,MAAM,CAAC,SAAS,EAAE,YAAY,CAAC,GAAG,QAAQ,CAAC,CAAC,CAAC,YAAY,CAAC,CAAC;IAE3D,MAAM,YAAY,GAAG,CAAC,CAAM,EAAE,QAAgB,EAAE,EAAE;QAChD,IAAI,QAAQ,EAAE,MAAM,GAAG,MAAM,EAAE;YAC7B,QAAQ,CAAC,mCAAmC,MAAM,cAAc,CAAC,CAAC;YAClE,YAAY,CAAC,IAAI,CAAC,CAAC;SAC